<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".emotional_support.FragmentEmotionalSupportAddMood">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/enter_nutrition_title_"
        android:layout_marginStart="16dp"
        android:textColor="@color/black"
        android:textSize="15sp"
        android:id="@+id/tv_mood_tracking"
        android:fontFamily="@font/poppins_medium"
        android:layout_marginTop="@dimen/size_margin_16"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_que_1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_margin_16"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/Emotional_Support_que1"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_mood_tracking" />

    <EditText
        android:id="@+id/et_ans_1"
        android:layout_width="match_parent"
        android:layout_height="38dp"
        android:layout_weight="1"
        android:background="@drawable/team_listing_item_border_color_primary"
        android:fontFamily="@font/poppins_regular"
        android:hint="@string/please_enter_answer"
        android:paddingStart="16dp"
        android:paddingTop="5dp"
        android:layout_marginTop="3dp"
        android:paddingEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:textColor="@color/black"
        android:textColorHint="@color/text_hint_color"
        android:textSize="@dimen/text_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_1"
        />

    <TextView
        android:id="@+id/tv_que_2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:layout_marginTop="@dimen/size_margin_16"

        android:fontFamily="@font/poppins_regular"
        android:text="@string/Emotional_Support_que2"
        android:textColor="@color/black"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_ans_1" />

    <EditText
        android:id="@+id/et_ans_2"
        android:layout_width="match_parent"
        android:layout_height="38dp"
        android:layout_weight="1"
        android:background="@drawable/team_listing_item_border_color_primary"
        android:fontFamily="@font/poppins_regular"
        android:hint="@string/please_enter_answer"
        android:paddingStart="16dp"
        android:paddingTop="5dp"
        android:layout_marginTop="3dp"
        android:paddingEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:textColor="@color/black"
        android:textColorHint="@color/text_hint_color"
        android:textSize="@dimen/text_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_2"
        />

    <TextView
        android:id="@+id/tv_que_3"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_margin_16"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/Emotional_Support_que3"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_ans_2" />

    <EditText
        android:id="@+id/et_ans_3"
        android:layout_width="match_parent"
        android:layout_height="38dp"
        android:layout_weight="1"
        android:background="@drawable/team_listing_item_border_color_primary"
        android:fontFamily="@font/poppins_regular"
        android:hint="@string/please_enter_answer"
        android:paddingStart="16dp"
        android:paddingTop="5dp"
        android:layout_marginTop="3dp"
        android:paddingEnd="16dp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        android:textColor="@color/black"
        android:textColorHint="@color/text_hint_color"
        android:textSize="@dimen/text_12"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_3"
        />

    <TextView
        android:id="@+id/tv_que_4"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_margin_16"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/Emotional_Support_que4"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/et_ans_3" />

    <Button
        android:id="@+id/btn_yes"
        android:layout_width="54dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_green_button"
        android:fontFamily="@font/poppins_regular"
        android:paddingTop="3dp"
        android:text="@string/yes"
        android:textColor="@color/white"
        android:textSize="12sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_4" />

    <Button
        android:layout_width="54dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_green_button"
        android:fontFamily="@font/poppins_regular"
        android:paddingTop="3dp"
        android:text="@string/no"
        android:id="@+id/btn_no"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/btn_yes"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_4" />

    <TextView
        android:id="@+id/tv_que_5"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_margin_16"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/Emotional_Support_que5"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_yes" />

    <Button
        android:id="@+id/btn_que_5_yes"
        android:layout_width="54dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_green_button"
        android:fontFamily="@font/poppins_regular"
        android:paddingTop="3dp"
        android:text="@string/yes"
        android:textColor="@color/white"
        android:textSize="12sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_5" />

    <Button
        android:layout_width="54dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_green_button"
        android:fontFamily="@font/poppins_regular"
        android:paddingTop="3dp"
        android:text="@string/no"
        android:id="@+id/btn_que_5_no"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/btn_que_5_yes"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_5" />
    <TextView
        android:id="@+id/tv_que_6"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/size_margin_16"
        android:fontFamily="@font/poppins_regular"
        android:text="@string/Emotional_Support_que6"
        android:textColor="@color/black"
        android:textSize="13sp"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_que_5_no" />
    <Button
        android:id="@+id/btn_que_6_yes"
        android:layout_width="54dp"
        android:layout_height="30dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_green_button"
        android:fontFamily="@font/poppins_regular"
        android:paddingTop="3dp"
        android:text="@string/yes"
        android:textColor="@color/white"
        android:textSize="12sp"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_6" />

    <Button
        android:layout_width="54dp"
        android:layout_height="30dp"
        android:layout_marginTop="8dp"
        android:background="@drawable/rounded_green_button"
        android:fontFamily="@font/poppins_regular"
        android:paddingTop="3dp"
        android:text="@string/no"
        android:id="@+id/btn_que_6_no"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:layout_marginStart="10dp"
        app:layout_constraintStart_toEndOf="@+id/btn_que_6_yes"
        app:layout_constraintTop_toBottomOf="@+id/tv_que_6" />

    <LinearLayout
        android:id="@+id/ll_buttons"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:paddingStart="16dp"
        android:paddingEnd="16dp"
        android:weightSum="2"
        android:paddingBottom="16dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_que_6_no">

        <Button
            android:id="@+id/et_btn_cancel"
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:layout_weight="1"
            android:background="@drawable/rounded_corner_btn_cancel"
            android:fontFamily="@font/poppins_regular"
            android:hint="@string/cancel"
            android:paddingStart="16dp"
            android:paddingTop="3dp"
            android:layout_margin="2dp"
            android:paddingEnd="16dp"
            android:textColor="@color/black"
            android:textColorHint="@color/black"
            android:textSize="10dp" />

        <Button
            android:id="@+id/et_submit"
            android:layout_width="match_parent"
            android:layout_height="38dp"
            android:layout_marginStart="3dp"
            android:layout_weight="1"
            android:background="@drawable/orange_rounded_btn_shape_15dp"
            android:fontFamily="@font/poppins_regular"
            android:hint="@string/submit"
            android:paddingStart="16dp"
            android:paddingTop="3dp"
            android:paddingEnd="16dp"
            android:layout_margin="2dp"
            android:textColor="@color/black"
            android:textColorHint="@color/white"
            android:textSize="10dp" />

    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>